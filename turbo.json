{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_DEFAULT_ROOT_COMPONENT"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "lib/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "deve": {
      "cache": false
    },
    "lint:check": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "serve": {
      "cache": false
    }
  }
}
